name: "text_normalization"
backend: "tensorrt"
platform: "tensorrt_plan"
max_batch_size: 8

input [
  {
    name: "input_ids"
    data_type: TYPE_INT64
    dims: [ -1 ]  # Variable sequence length
  },
  {
    name: "attention_mask"
    data_type: TYPE_INT64
    dims: [ -1 ]  # Variable sequence length
  }
]

output [
  {
    name: "encoder_last_hidden_state"
    data_type: TYPE_FP32
    dims: [ -1, 1024 ]  # [sequence_length, hidden_size]
  },
  {
    name: "logits"
    data_type: TYPE_FP32
    dims: [ -1, -1, 50364 ]  # [sequence_length, batch_size, vocab_size]
  }
]

instance_group [
  {
    count: 1
    kind: KIND_GPU
    gpus: [ 0 ]
  }
]

dynamic_batching {
  preferred_batch_size: [ 4, 8 ]
  max_queue_delay_microseconds: 100
}

optimization {
  execution_accelerators {
    gpu_execution_accelerator: [
      {
        name: "tensorrt"
        parameters { 
          key: "precision_mode" 
          value: "FP16" 
        }
        parameters { 
          key: "max_workspace_size_bytes" 
          value: "1073741824"  # 1GB
        }
      }
    ]
  }
} 